{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    
    {
      "source": "/send-email",
      "destination": "/server.js"
    },
   
    {
      "source": "/(.*)\\.(css|js|jpg|png|svg|ico)",
      "destination": "/$1.$2"
    },
  
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
